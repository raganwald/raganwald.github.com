---
layout: nil
---
<?xml version="1.0"?>
<feed xmlns="http://www.w3.org/2005/Atom">
 
  <title>raganwald.com</title>
  <link href="http://raganwald.com/"/>
  <link type="application/atom+xml" rel="self" href="http://raganwald.com/atom.xml"/>
  <updated>{{ site.time | date_to_xmlschema }}</updated>
  <id>http://raganwald.com/</id>
  <author>
    <name>Reginald Braithwaite</name>
    <email>reg@braythwayt.com</email>
  </author>

  {% for post in site.posts %}
    {% unless post.categories contains "noindex" or post.categories contains "posterous" or post.categories contains "homoiconic" %}
      <entry>
        <id>http://raganwald.com{{ post.id }}</id>
        <link type="text/html" rel="alternate" href="http://raganwald.com{{ post.url }}"/>
        <title>{{ post.title }}</title>
        <published>{{ post.date | date_to_xmlschema }}</published>
        <updated>{{ post.date | date_to_xmlschema }}</updated>
        <author>
          <name>Reginald Braithwaite</name>
          <uri>http://raganwald.com/</uri>
        </author>
        <content type="html">{{ post.content | xml_escape }}</content>
      </entry>
    {% endunless %}
  {% endfor %}
 
</feed>